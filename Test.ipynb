   "source": [
    "def factorial(n):return reduce(lambda x,y:x*y,[1]+range(1,n+1))\n",
    "\n",
    "def factorial(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n-1)"
   ]
